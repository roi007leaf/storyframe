{
  "id": "storyframe",
  "title": "Storyframe",
  "description": "GM journal reader with synchronized speaker portraits for players",
  "version": "1.0.3",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "RoiLeaf",
      "discord": "RoiLeaf",
      "flags": {}
    }
  ],
  "esmodules": [
    "storyframe.mjs"
  ],
  "styles": [
    "styles/design-tokens.css",
    "styles/gm-interface.css",
    "styles/gm-sidebar.css",
    "styles/player-viewer.css"
  ],
  "socket": true,
  "relationships": {
    "requires": [
      {
        "id": "socketlib",
        "type": "module"
      }
    ]
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "flags": {
    "canUpload": true
  },
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#"
}
